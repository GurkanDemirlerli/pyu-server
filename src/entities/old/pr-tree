import { ViewEntity, ViewColumn } from 'typeorm';

@ViewEntity('new_view')
export class PrTreeViewEntity {

    @ViewColumn()
    project_id: number;

    @ViewColumn()
    project_title: string;

    @ViewColumn()
    project_status_id: number;

    @ViewColumn()
    project_status_title: string;

    @ViewColumn()
    project_status_task_id: number;

    @ViewColumn()
    project_status_task_title: string;

}