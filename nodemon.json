{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules"],
  "verbose": true,
  "execMap":{
    "ts":"node --inspect=1923 --require ts-node/register"
  },
  "watch": ["src/**/*"],
  "env": {
    "NODE_ENV": "development",
    "TS_NODE_TRANSPILE_ONLY": true,
    "TS_NODE_FILES":true
  },
  "ext": "js,json,ts",
  "inspect": true
}
